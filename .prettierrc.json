{
  "proseWrap": "always",
  "parser": "typescript",
  "tabWidth": 4,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "semi": true,
  "bracketSameLine": false,
  "jsxSingleQuote": false,
  "endOfLine": "auto",
  "printWidth": 120,
  "experimentalTernaries": false
}
